<div class="container">
  <div class="row">
    <div class="col-sm-12 col-md-8 col-lg-5 px-5 px-sm-0 mx-auto">
      <h2>商品新規登録</h2>
      <%= form_with model: @product, url: "/admin/products", local:true do |f| %>
        <%= render "form", f: f %>
        <div class="form-group">
          <%= f.submit :新規登録, class: 'btn btn-success' %>
        </div>
      <% end %>

    </div>
  </div>
</div>


